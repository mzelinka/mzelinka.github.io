	pages = {4513--4558},
	file = {Full Text PDF:/Users/zelinka1/Zotero/storage/V8Z7RPB6/Sellar et al. - 2019 - UKESM1 Description and Evaluation of the U.K. Ear.pdf:application/pdf;Snapshot:/Users/zelinka1/Zotero/storage/5AFVYVMN/2019MS001739.html:text/html},
