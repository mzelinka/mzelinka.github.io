@article{goode_earths_2021,
	title = {Earth's {Albedo} 1998–2017 as {Measured} {From} {Earthshine}},
	volume = {48},
	issn = {1944-8007},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1029/2021GL094888},
	doi = {10.1029/2021GL094888},
	abstract = {The reflectance of the Earth is a fundamental climate parameter that we measured from Big Bear Solar Observatory between 1998 and 2017 by observing the earthshine using modern photometric techniques to precisely determine daily, monthly, seasonal, yearly and decadal changes in terrestrial albedo from earthshine. We find the inter-annual fluctuations in albedo to be global, while the large variations in albedo within individual nights and seasonal wanderings tend to average out over each year. We measure a gradual, but climatologically significant 0.5 decline in the global albedo over the two decades of data. We found no correlation between the changes in the terrestrial albedo and measures of solar activity. The inter-annual pattern of earthshine fluctuations are in good agreement with those measured by CERES (data began in 2001) even though the satellite observations are sensitive to retroflected light while earthshine is sensitive to wide-angle reflectivity. The CERES decline is about twice that of earthshine.},
	language = {en},
	number = {17},
	urldate = {2021-10-09},
	journal = {Geophysical Research Letters},
	author = {Goode, P. R. and Pallé, E. and Shoumko, A. and Shoumko, S. and Montañes-Rodriguez, P. and Koonin, S. E.},
	year = {2021},
	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2021GL094888},
	keywords = {atmospheres, methods, observational, planetary systems, planets and satellites, spectroscopic techniques, stars: low mass},
	pages = {e2021GL094888},
	file = {Snapshot:/Users/zelinka1/Zotero/storage/FB92J3SB/2021GL094888.html:text/html},
}

