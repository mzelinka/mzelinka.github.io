	author = {Suzuki, Kentaroh and Golaz, Jean-Christophe and Stephens, Graeme L.},
	year = {2013},
	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1002/grl.50874},
	keywords = {model evaluation, aerosol indirect effect, cloud microphysics},
	pages = {4464--4468},
	file = {Snapshot:/Users/zelinka1/Zotero/storage/TPSVM9DW/grl.html:text/html},
}

@article{jing_impact_2018,
	title = {The {Impact} of {Process}-{Based} {Warm} {Rain} {Constraints} on the {Aerosol} {Indirect} {Effect}},
	volume = {45},
	issn = {1944-8007},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2018GL079956},
	doi = {10.1029/2018GL079956},
	abstract = {Many global climate models have been found to generate warm rain too frequently. This study, with a particular global climate model, found that the problem can be mitigated via altering the autoconversion process so as to inhibit rain formation under conditions of a large cloud number concentration and small droplet sizes. However, this improvement was found to cause an overly large aerosol indirect effect. This dichotomy between the constraint on warm-rain formation process and the energy-budget requirement on aerosol indirect effect was found to result from a pronounced cloud-water response to aerosol perturbations, which is amplified through the wet scavenging feedback to an extent depending on precipitation-formation parameterization. Thus, critical compensating errors exist between warm-rain formation and other key processes, and better constraint on these processes, particularly wet scavenging, is required to mitigate the dichotomy. The results have broad implication for models that suffer from the aforementioned too-frequent warm-rain formation bias.},
	language = {en},
	number = {19},
	urldate = {2021-07-26},
	journal = {Geophysical Research Letters},
	author = {Jing, Xianwen and Suzuki, Kentaroh},
	year = {2018},
	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2018GL079956},
	keywords = {cloud, climate model, aerosol indirect effect, energy balance, warm rain},
	pages = {10,729--10,737},
	file = {Snapshot:/Users/zelinka1/Zotero/storage/MPZELPRG/2018GL079956.html:text/html},
}

